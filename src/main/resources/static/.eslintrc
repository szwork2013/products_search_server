{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "ecmaFeatures": {
        "forOf": true,
        "jsx": true,
        "es6": true
    },
    "rules": {
        "object-curly-spacing": 0,
        "react/no-multi-comp": [0, { "ignoreStateless": true }],
        "react/prefer-es6-class": [2, "never"],
        "no-console": 0,
        "comma-dangle": 0,
        "indent": [2, 2, {"SwitchCase": 1}],
        "react/prop-types": 0
    }
}
